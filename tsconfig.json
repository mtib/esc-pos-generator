{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "module": "es6",
        "moduleResolution": "node",
        "lib": [
            "DOM",
            "es2020"
        ],
        "target": "es5",
        "allowJs": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "jsx": "react",
    },
    "include": [
        "./src/**/*"
    ]
}
